<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--属性集合-->
    <declare-styleable name="SearchView" >
        <!--hint-->
        <attr name="hint" format="string"/>
        <!--搜索图标-->
        <attr name="search_ico" format="reference" />
        <!--清空图标-->
        <attr name="clear_ico" format="reference" />
        <!--语音图标-->
        <attr name="voice_ico" format="reference" />
    </declare-styleable>

    <declare-styleable name="VerifiedButton">
        <attr name="totalSeconds" format="integer"/>
        <attr name="intervalSeconds" format="integer"/>
        <attr name="normalBackground" format="color" />
        <attr name="verifiedBackground" format="color" />
    </declare-styleable>

</resources>